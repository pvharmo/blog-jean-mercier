{
  "name": "themovieshrinkblog",
  "description": "A blog on the hidden meaning of contemporary films",
  "version": "2.0.0",
  "author": "Jonathan Caron-Roberge",
  "private": true,
  "scripts": {
    "before-build": "node -r esm ./build-tools/beforeBuild.js",
    "after-build": "node -r esm ./build-tools/afterBuild.js",
    "dev": "npm run before-build && sapper dev && npm run after-build",
    "build": "sapper build --legacy",
    "export": "npm run before-build && sapper export --legacy && npm run after-build",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bulma": "^0.9.1",
    "compression": "^1.7.1",
    "marked": "^1.2.7",
    "netlify-cms": "^2.10.84",
    "polka": "next",
    "postcss": "^8.2.3",
    "rollup-plugin-css-only": "^3.1.0",
    "sass": "^1.32.2",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "autoprefixer": "^10.2.1",
    "disqus-svelte": "^1.0.9",
    "esm": "^3.2.25",
    "ncp": "^2.0.0",
    "node-sass": "^7.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-root-import": "^1.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-preprocess": "^4.6.1"
  }
}
